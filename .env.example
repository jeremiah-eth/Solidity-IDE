# QuickNode Multichain RPC (ONE URL for all chains!)
VITE_QUICKNODE_URL=https://YOUR-ENDPOINT.quiknode.pro/YOUR-KEY/

# Block Explorer API (ONE KEY for all EVM chains!)
VITE_ETHERSCAN_API_KEY=your_etherscan_api_key

# IPFS Storage - Pinata Cloud
VITE_PINATA_API_KEY=your_pinata_api_key
VITE_PINATA_SECRET_KEY=your_pinata_secret_key

# Optional: Custom RPC fallbacks
VITE_ENABLE_FALLBACK_RPC=true
